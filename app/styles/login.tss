".logincontainer": {
	backgroundColor: 'rgba(0,0,0,0.7)'
},
"#wrapper": {
	backgroundColor: "#fff",
	borderRadius: 8,
	borderColor: '#000',
	borderWidth: 2,
	width: 300,
	height: 300,
	top: 20,
	layout: 'vertical'
},
"#wrapper[platform=android]": {
	width: '75%',
	height: '50%'
},
"#img": {
	top: 10,
	width:100
},
"TextField": {
	backgroundColor: "#fff",
	left: 10, right: 10,
	height: '40dp',
	borderColor: '#888',
	top: 10
},
"#name": {
	top: 30
},
"#password": {
	passwordMask: true,
	autocorrection: false,
	autocapitalization: Ti.UI.TEXT_AUTOCAPITALIZATION_NONE
}
"#submit": {
	title: 'Create Account',
	backgroundColor: '#fff',
	left: 10, right: 10,
	height: '40dp',
	top: 10
}